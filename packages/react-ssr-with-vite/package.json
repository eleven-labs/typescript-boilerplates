{
  "name": "react-ssr-with-vite",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "ts-node": "ts-node -r dotenv/config",
    "build": "yarn ts-node bin/build",
    "prerender": "yarn build && yarn ts-node bin/prerender",
    "start:dev": "yarn ts-node bin/dev",
    "start:prod": "NODE_ENV=production node dist/server",
    "start:static": "npx serv --path dist/public"
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --fix"
    ]
  },
  "engines": {
    "npm": "please-use-yarn-instead",
    "yarn": ">= 1.2.0",
    "node": ">= 16.0"
  },
  "dependencies": {
    "@eleven-labs/design-system": "^0.3.1",
    "express": "^4.18.2",
    "hoofd": "^1.5.2",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-middleware": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-router-dom": "^6.8.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@elevenlabs/eslint-config": "^0.0.1",
    "@types/express": "^4.17.17",
    "@types/i18next": "^13.0.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-i18next": "^8.1.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.7.1",
    "sass": "^1.58.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
